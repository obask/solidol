{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "Node",
    "strict": true,
    "sourceRoot": "src",
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "dom/*",
    "jsx-runtime/*",
    "reactive/*",
    "render/*",
    "web/*",
  ]
}